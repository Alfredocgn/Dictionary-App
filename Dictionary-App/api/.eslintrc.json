{
  "extends": ["standard", "plugin:@typescript-eslint/recommended"],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "rules": {
    "semi": "off",
    "max-len": "off",
    "no-multiple-empty-lines": "off",
    "quotes": "off"
  }
}
